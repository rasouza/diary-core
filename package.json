    "awilix": "^4.2.6",
    "dotenv": "^8.2.0",
    "fastify": "^3.9.2",
    "fastify-healthcheck": "^3.1.0",
    "fastify-helmet": "^5.1.0",
    "fastify-sentry": "^1.4.0",
    "fastify-static": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash-fp": "^0.10.4",
    "luxon": "^1.25.0",
    "make-promises-safe": "^5.1.0",
